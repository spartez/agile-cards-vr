{
    "expressErrorHandling": false,
    "development": {
        "port": 3000,
        "store": {
            "adapter": "jugglingdb",
            "type": "sqlite3",
            "database": "store.db"
        }
    },
    "production": {
        "port": "$PORT",
        "localBaseUrl": "https://your-subdomain.herokuapp.com",
        "store": {
            "type": "postgres",

            "url": "$DATABASE_URL"
        },
        "whitelist": ["*.jira-dev.com", "*.atlassian.net", "*.jira.com"]
    }
}
